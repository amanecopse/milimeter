<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#7A8B6F"
    >


    <com.dinuscxj.progressbar.CircleProgressBar
        android:id="@+id/lt_graph"
        android:layout_width="177dp"
        android:layout_height="177dp"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="36dp" />

    <com.dinuscxj.progressbar.CircleProgressBar
        android:id="@+id/gs_graph"
        android:layout_width="177dp"
        android:layout_height="177dp"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="236dp" />

    <com.dinuscxj.progressbar.CircleProgressBar
        android:id="@+id/esmd_graph"
        android:layout_width="177dp"
        android:layout_height="177dp"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="436dp" />

    <TextView
        android:id="@+id/lt_goal"
        android:layout_width="90dp"
        android:layout_height="62dp"
        android:text="목표등급 :\n현재등급 :"
        android:textColor="#FF1F4C21"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="226dp"
        tools:layout_editor_absoluteY="117dp" />

    <TextView
        android:id="@+id/gs_goal"
        android:layout_width="90dp"
        android:layout_height="62dp"
        android:text="목표등급 :\n현재등급 :"
        android:textColor="#FF1F4C21"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="226dp"
        tools:layout_editor_absoluteY="317dp" />

    <TextView
        android:id="@+id/esmd_goal"
        android:layout_width="90dp"
        android:layout_height="62dp"
        android:text="목표등급 :\n현재등급 :"
        android:textColor="#FF1F4C21"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="226dp"
        tools:layout_editor_absoluteY="517dp" />

    <TextView
        android:id="@+id/lt"
        android:layout_width="90dp"
        android:layout_height="42dp"
        android:text="레그턱"
        android:textColor="#FF1F4C21"
        android:textSize="30sp"
        tools:layout_editor_absoluteX="226dp"
        tools:layout_editor_absoluteY="64dp" />

    <TextView
        android:id="@+id/gs"
        android:layout_width="181dp"
        android:layout_height="42dp"
        android:text="전장순환운동"
        android:textColor="#FF1F4C21"
        android:textSize="30sp"
        tools:layout_editor_absoluteX="226dp"
        tools:layout_editor_absoluteY="264dp" />

    <TextView
        android:id="@+id/gs2"
        android:layout_width="170dp"
        android:layout_height="42dp"
        android:text="240m달리기"
        android:textColor="#FF1F4C21"
        android:textSize="30sp"
        tools:layout_editor_absoluteX="226dp"
        tools:layout_editor_absoluteY="464dp" />

    <ImageButton
        android:id="@+id/home"
        android:layout_width="81dp"
        android:layout_height="81dp"
        android:background="@android:color/transparent"
        android:scaleType="center"
        android:src="@drawable/home_50_white"
        tools:layout_editor_absoluteY="650dp" />

    <ImageButton
        android:id="@+id/body"
        android:layout_width="81dp"
        android:layout_height="81dp"
        android:background="@android:color/transparent"
        android:scaleType="center"
        android:src="@drawable/body_50_white"
        tools:layout_editor_absoluteX="81dp"
        tools:layout_editor_absoluteY="650dp" />

    <ImageButton
        android:id="@+id/result"
        android:layout_width="81dp"
        android:layout_height="81dp"
        android:background="@android:color/transparent"
        android:scaleType="center"
        android:src="@drawable/result_50_white"
        tools:layout_editor_absoluteX="165dp"
        tools:layout_editor_absoluteY="650dp" />

    <ImageButton
        android:id="@+id/goal"
        android:layout_width="81dp"
        android:layout_height="81dp"
        android:background="@android:color/transparent"
        android:scaleType="center"
        android:src="@drawable/goal_50_white"
        tools:layout_editor_absoluteX="246dp"
        tools:layout_editor_absoluteY="650dp" />

    <ImageButton
        android:id="@+id/setting"
        android:layout_width="81dp"
        android:layout_height="81dp"
        android:background="@android:color/transparent"
        android:scaleType="center"
        android:src="@drawable/setting_50_white"
        tools:layout_editor_absoluteX="327dp"
        tools:layout_editor_absoluteY="650dp" />

</androidx.constraintlayout.widget.ConstraintLayout>