<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#7A8B6F"
    tools:context=".MainActivity">

    <ImageButton
        android:id="@+id/home"
        android:layout_width="81dp"
        android:layout_height="81dp"
        android:background="@android:color/transparent"
        android:scaleType="center"
        android:src="@drawable/home_50_white"
        tools:layout_editor_absoluteY="650dp" />

    <ImageButton
        android:id="@+id/body"
        android:layout_width="81dp"
        android:layout_height="81dp"
        android:background="@android:color/transparent"
        android:scaleType="center"
        android:src="@drawable/body_50_white"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:layout_editor_absoluteX="81dp" />

    <ImageButton
        android:id="@+id/result"
        android:layout_width="81dp"
        android:layout_height="81dp"
        android:background="@android:color/transparent"
        android:scaleType="center"
        android:src="@drawable/result_50_white"
        tools:layout_editor_absoluteX="165dp"
        tools:layout_editor_absoluteY="650dp" />

    <ImageButton
        android:id="@+id/goal"
        android:layout_width="81dp"
        android:layout_height="81dp"
        android:background="@android:color/transparent"
        android:scaleType="center"
        android:src="@drawable/goal_50_white"
        tools:layout_editor_absoluteX="246dp"
        tools:layout_editor_absoluteY="650dp" />

    <ImageButton
        android:id="@+id/setting"
        android:layout_width="81dp"
        android:layout_height="81dp"
        android:background="@android:color/transparent"
        android:scaleType="center"
        android:src="@drawable/setting_50_white"
        tools:layout_editor_absoluteX="327dp"
        tools:layout_editor_absoluteY="650dp" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:layout_width="248dp"
        android:layout_height="207dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:src="@drawable/face"
        tools:layout_editor_absoluteX="81dp"
        tools:layout_editor_absoluteY="121dp" />

    <TextView
        android:layout_width="251dp"
        android:layout_height="179dp"
        android:layout_align
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:background="#ACB391"
        tools:layout_editor_absoluteX="80dp"
        tools:layout_editor_absoluteY="358dp"

    <Button
        android:id="@+id/button"
        android:layout_width="129dp"
        android:layout_height="59dp"
        android:backgroundTint="#ACB391"
        android:text="D-365"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="266dp"
        tools:layout_editor_absoluteY="16dp" />

</androidx.constraintlayout.widget.ConstraintLayout>